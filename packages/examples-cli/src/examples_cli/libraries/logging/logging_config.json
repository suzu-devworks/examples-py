{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": { "format": "[JSON] %(asctime)s [%(levelname)-5s] %(message)s" }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "simple"
    }
  },
  "loggers": {
    "": {
      "level": "DEBUG",
      "handlers": ["console"]
    }
  }
}
